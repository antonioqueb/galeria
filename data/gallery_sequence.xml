<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Secuencia para Gallery Share -->
        <record id="seq_gallery_share" model="ir.sequence">
            <field name="name">Secuencia Galería</field>
            <field name="code">gallery.share</field>
            <field name="prefix">CAT/%(year)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <!-- Importante: eval="False" permite que la secuencia funcione para todas las compañías -->
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>